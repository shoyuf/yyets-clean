{
  "name": "yyets-clean",
  "version": "0.1.0",
  "private": true,
  "description": "A electron client based on YYETS API",
  "author": "Shoyuf <shoyuf@shoyuf.top> (https://shoyuf.top)",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:electron": "vue-cli-service build:electron",
    "serve:electron": "vue-cli-service serve:electron",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "element-ui": "^2.4.7",
    "qs": "^6.5.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-e2e-cypress": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-unit-mocha": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-airbnb": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "chai": "^4.1.2",
    "electron": "^2.0.7",
    "electron-builder": "^20.28.1",
    "electron-devtools-installer": "^2.2.4",
    "vue-cli-plugin-electron-builder": "^1.0.0-beta.8",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "max-len": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "chrome 61"
  ],
  "bugs": {
    "url": "https://github.com/shoyuf/yyets-clean/issues",
    "email": "shoyuf@shoyuf.top"
  },
  "build": {
    "productName": "yyets-clean",
    "appId": "com.zimuzu.electron"
  },
  "dmg": {
    "contents": [
      {
        "x": 410,
        "y": 150,
        "type": "link",
        "path": "/Applications"
      },
      {
        "x": 130,
        "y": 150,
        "type": "file"
      }
    ]
  },
  "engines": {
    "node": "8"
  },
  "license": "MIT",
  "linux": {
    "icon": "build"
  },
  "mac": {
    "icon": "build/icon.icns"
  },
  "main": "dist_electron/bundled/background.js",
  "win": {
    "icon": "build/icon.ico"
  }
}
